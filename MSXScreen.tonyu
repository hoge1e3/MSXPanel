}
function showKeys() {
    print(p.platform.machine.inputs[24]);
}
function pokey(name,val){
    let k=keynames [name];
    let bit=k>>8;
    let ad=(k&255);
    let cur=keymat;
    if(val){
        keymat=cur | bit;
    }
}

\showPad() {
    $pad=new Pad {
        buttonCnt=2
    };
}
\hidePad() {
    if ($pad) {
        $pad.die();    
        $pad=null;
    }
}
